#include <iostream>

using namespace std;

int main()
{
    long long n , b , d , cont = 0 , s = 0;
    cin>>n>>b>>d;
    for(int i = 0 ; i < n ; i++)
        {
            int a = 0 ;
            cin>>a;
            if(a<=b){
            s=s+a;
            }

            if(s>d)
                {
                    cont++;
                    s=0;
                }

        }

        cout<<cont;
    return 0;
}
